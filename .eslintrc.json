{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "plugin:react/recommended",
    "@wemake-services/javascript"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "react/prop-types": 0,
    "unicorn/prefer-export-from": 0,
    "unicorn/filename-case": 0,
    "unicorn/consistent-function-scoping": 0,
    "unicorn/no-null": 0,
    "no-trailing-spaces": 0,
    "quote-props": 0,
    "dot-notation": 0,
    "multiline-ternary": 0
  }
}
